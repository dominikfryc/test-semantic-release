{
  "name": "test-semantic-release-package",
  "version": "0.0.0",
  "description": "Web Components UI Library",
  "keywords": [
    "web components",
    "ui library",
    "lit",
    "vite",
    "typescript"
  ],
  "homepage": "https://github.com/dominikfryc/test-semantic-release#readme",
  "bugs": "https://github.com/dominikfryc/test-semantic-release/issues",
  "repository": "github:dominikfryc/test-semantic-release",
  "license": "MIT",
  "author": "Dominik Fryƒç <fryc.dominik@gmail.com> (https://dominikfryc.com/)",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./element": {
      "types": "./dist/my-element/my-element.d.ts",
      "import": "./dist/my-element/my-element.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "release": "semantic-release"
  },
  "dependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "glob": "^10.3.10",
    "semantic-release": "^22.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
